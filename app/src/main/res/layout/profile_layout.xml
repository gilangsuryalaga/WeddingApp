<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <RelativeLayout
        android:id="@+id/fotoProfile"
        android:layout_width="380dp"
        android:layout_height="wrap_content"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="16dp">
        <ImageView
            android:id="@+id/foto"
            android:layout_width="match_parent"
            android:layout_height="205dp"
            android:background="@drawable/weddingpewds"
            />
        <TextView
            android:layout_marginTop="220dp"
            android:id="@+id/textJalan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="JL.laksjlakjsd"
            />
        <TextView
            android:layout_marginTop="240dp"
            android:id="@+id/textKab"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Kabupaten asklaksdjlaksjd"
            />
        <TextView
            android:layout_marginTop="260dp"
            android:id="@+id/textKota"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Kota Bandung"
            />
    </RelativeLayout>
            <androidx.cardview.widget.CardView
                android:layout_marginTop="30dp"
                app:layout_constraintTop_toBottomOf="@id/fotoProfile"
                android:layout_width="60dp"
                android:layout_height="60dp"
                app:cardCornerRadius="10dp"
                tools:ignore="MissingConstraints">

                <ImageView
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/wedding1"
                    />
            </androidx.cardview.widget.CardView>


</androidx.constraintlayout.widget.ConstraintLayout>
